{
  "manifest_version": 3,
  "name": "Coderunner Autocomplete Enabler",
  "version": "1.0",
  "description": "Enables autocomplete in the ace editor when the page is loaded.",
  "permissions": ["activeTab"],
  "icons": {
    "48": "icon.jpg"
  },
  "browser_action": {
    "default_icon": {
      "48": "icon.jpg"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://cloudrunner.auckland.ac.nz/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["script.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
