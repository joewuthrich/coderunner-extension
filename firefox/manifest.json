{
  "manifest_version": 2,
  "name": "Coderunner Autocomplete Enabler",
  "version": "1.0",
  "description": "Enables autocomplete in the ace editor when the page is loaded.",
  "permissions": ["activeTab"],
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icon.png"
    }
  },
  "content_security_policy": "script-src 'self' 'https://ace.c9.io/'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://ace.c9.io/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["script.js"]
}
